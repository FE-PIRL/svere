{
  "name": "@svere/cli",
  "version": "1.0.1",
  "description": "svere CLI",
  "types": "build/types/types.d.ts",
  "bin": {
    "svere": "bin/svere-cli"
  },
  "scripts": {
    "format": "prettier --write {src,src/**,__tests__}/*.{js,ts,tsx,json}",
    "clean-build": "npx tsc --build --clean && rm -rf ./build",
    "compile": "npx tsc --build --verbose --pretty",
    "start": "npm run compile -- --watch",
    "build": "npm run format && npm run clean-build && npm run compile",
    "prepublishOnly": "npm run build",
    "release:patch": "npm version patch && npm publish",
    "test": "npx jest",
    "snapupdate": "npx jest --updateSnapshot",
    "coverage": "npx jest --coverage"
  },
  "files": [
    "tsconfig.json",
    "build",
    "LICENSE",
    "readme.md",
    "docs",
    "bin"
  ],
  "author": "benyasin",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/FE-PIRL/svere.git",
    "templates": "https://github.com/FE-PIRL/svere-templates.git"
  },
  "dependencies": {
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-replace": "^2.4.2",
    "asyncro": "^3.0.0",
    "camelcase": "^6.2.0",
    "commander": "^6.2.1",
    "degit": "^2.8.0",
    "fs-extra": "^9.1.0",
    "gluegun": "latest",
    "jpjs": "^1.2.1",
    "node-sass": "^6.0.0",
    "rollup": "^2.50.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-svelte": "^7.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "svelte": "^3.38.2",
    "svelte-loader": "^3.1.1",
    "svelte-preprocess": "^4.7.3",
    "tiny-glob": "^0.2.9"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.8",
    "@types/jest": "^26.0.19",
    "@types/node": "^12.7.11",
    "@types/yargs-parser": "^20.2.0",
    "execa": "^4.0.3",
    "jest": "^26.2.2",
    "jest-specific-snapshot": "^4.0.0",
    "kleur": "^4.1.1",
    "open": "^7.0.4",
    "prettier": "^1.12.1",
    "rimraf": "^3.0.0",
    "ts-jest": "^26.5.6",
    "ts-node": "^8.4.1",
    "typescript": "^4.2.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "gitHead": "2153d2978edd62553cfe1351a6233d5e586ed56b"
}
