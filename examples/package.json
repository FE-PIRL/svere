    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/svelte": "^3.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "@types/jest": "^26.0.14",
    "jest": "^26.6.3",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "svelte": "^3.37.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "ts-jest": "^26.5.5",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  },
  "jest": {
    "transform": {
      "^.+\\.svelte$": [
